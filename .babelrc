{
  "stage": 2,
  "env": {
    "development": {
      "plugins": [
        "react-transform"
      ],
      "extra": {
        "react-transform": {
          "transforms": [
            {
              "transform": "react-transform-hmr",
              "imports": [
                "react"
              ],
              "locals": [
                "module"
              ]
            }
          ]
        }
      }
    }
  },
  "optional": [
    "es7.classProperties",
    "es7.functionBind"
  ]
}